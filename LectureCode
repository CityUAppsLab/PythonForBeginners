#Add and remove elements from a list
sample_list = [1,2,3,4,5,6,7]
print(sample_list)
n = sample_list.pop(1)
print(sample_list)
sample_list.append(n)
print(sample_list)

#Result:
#[1,2,3,4,5,6,7]
#[1,3,4,5,6,7]
#[1,3,4,5,6,7,2]
